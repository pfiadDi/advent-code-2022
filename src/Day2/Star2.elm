module Day2.Star2 exposing (..)

import Html exposing (Html, text)


winner : ( Symbol, Result ) -> Int
winner round =
    case round of
        ( Rock, Draw ) ->
            3 + symbolToPts Rock

        ( Rock, Win ) ->
            6 + symbolToPts Paper

        ( Rock, Loss ) ->
            0 + symbolToPts Scissors

        ( Paper, Draw ) ->
            3 + symbolToPts Paper

        ( Paper, Win ) ->
            6 + symbolToPts Scissors

        ( Paper, Loss ) ->
            0 + symbolToPts Rock

        ( Scissors, Draw ) ->
            3 + symbolToPts Scissors

        ( Scissors, Win ) ->
            6 + symbolToPts Rock

        ( Scissors, Loss ) ->
            0 + symbolToPts Paper

        _ ->
            -100000


type Symbol
    = Rock
    | Paper
    | Scissors
    | Wrong


meToSymbol : String -> Result
meToSymbol val =
    case val of
        "X" ->
            Loss

        "Y" ->
            Draw

        "Z" ->
            Win

        _ ->
            WrongRes


symbolToPts : Symbol -> Int
symbolToPts symbol =
    case symbol of
        Rock ->
            1

        Paper ->
            2

        Scissors ->
            3

        Wrong ->
            -1000000


type Result
    = Win
    | Loss
    | Draw
    | WrongRes


resultToPts : Result -> Int
resultToPts res =
    case res of
        Win ->
            6

        Loss ->
            0

        Draw ->
            3

        WrongRes ->
            -100000000


elfToSymbol : String -> Symbol
elfToSymbol val =
    case val of
        "A" ->
            Rock

        "B" ->
            Paper

        "C" ->
            Scissors

        _ ->
            Wrong


main : Html msg
main =
    input
        |> String.split "\n"
        |> List.map stringToRound
        |> roundToPts
        |> List.sum
        |> Debug.toString
        |> text


roundToPts : List ( Symbol, Result ) -> List Int
roundToPts list =
    List.map getResult list


stringToRound : String -> ( Symbol, Result )
stringToRound round =
    String.split " " round
        |> (\x ->
                case x of
                    a :: b ->
                        ( elfToSymbol a, meToSymbol (List.head b |> Maybe.withDefault "Wrong") )

                    other ->
                        ( Wrong, WrongRes )
           )


getResult : ( Symbol, Result ) -> Int
getResult round =
    winner round


